<FindBugsFilter>
	<Match>
		<Package name="protobuf.*" />
	</Match>
	<Match>
		<Bug pattern="SE_NO_SERIALVERSIONID" />
	</Match>
	<Match>
		<Bug pattern="LI_LAZY_INIT_UPDATE_STATIC" />
		<Class name="coursesketch.database.util.institution.mongo.MongoInstitution" />
	</Match>

	<Match>
		<Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD" />
		<Or>
			<Class name="coursesketch.database.util.institution.mongo.MongoInstitution" />
			<Class name="coursesketch.database.util.user.UserClient" />
		</Or>
	</Match>


	<!-- General bug excludes -->
	<Match>
		<Bug pattern="SE_BAD_FIELD" />
	</Match>

	<Match>
		<Bug pattern="SE_NO_SERIALVERSIONID" />
	</Match>

	<Match>
		<Package name="~protobuf[.]srl[.].*?" />
	</Match>

	<Match>
		<Class name="~.*cobertura.*" />
	</Match>

	<Match>
		<Package name="~.*cobertura.*" />
	</Match>

	<Match>
		<Method name="~.*cobertura.*" />
	</Match>

	<Match>
		<Local name="~.*cobertura.*" />
	</Match>

	<Match>
		<Field name="~.*cobertura.*" />
	</Match>

	<Match>
		<Or>
			<Field name="e" />
			<Local name="e" />
		</Or>
		<Bug pattern="DLS_DEAD_LOCAL_STORE" />
	</Match>

	<!-- All bugs in test classes, except for JUnit-specific bugs -->
	<Match>
		<Class name="~.*\.*Test" />
		<Not>
			<Bug code="IJU" />
		</Not>
	</Match>

    <!--PROJECT SPECIFIC BUGS -->

    <Match>
        <Class name="coursesketch.database.util.institution.mongo.MongoInstitution" />
        <!-- problems with the double checked locking pattern -->
        <Bug pattern="NP_LOAD_OF_KNOWN_NULL_VALUE" />
    </Match>

    <Match>
        <Class name="coursesketch.database.util.user.UserClient" />
        <!-- problems with the double checked locking pattern -->
        <Bug pattern="NP_LOAD_OF_KNOWN_NULL_VALUE" />
    </Match>

    <Match>
        <Class name="coursesketch.database.util.PasswordHash" />
    </Match>
</FindBugsFilter>
